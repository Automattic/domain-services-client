{
  "name": "automattic/domain-services-client",
  "description": "Client library for Automattic's Domain Services API.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Automattic",
      "homepage": "https://automattic.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-client-implementation": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "http-interop/http-factory-guzzle": "^1.0",
    "php-http/mock-client": "^1.0",
    "automattic/phpdoc-markdown": "dev-add/remove-empty-references"
  },
  "autoload": {
    "classmap": [
      "lib/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "test/"
    ]
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Automattic/phpdoc-markdown.git"
    }
  ]
}
